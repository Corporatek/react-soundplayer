{
  "name": "react-soundplayer",
  "version": "0.1.0",
  "description": "Create custom SoundCloud players with React",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "prebuild": "rm -rf lib",
    "build-js": "babel src -d lib --experimental",
    "build-css": "npm run cssnext cssnext/main.css css/react-soundplayer.css",
    "build": "npm run build-js",
    "prepublish": "npm run build"
  },
  "author": "Dmitri Voronianski <dmitri.voronianski@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "object-assign": "^2.0.0",
    "react": ">=0.12.2",
    "soundcloud-audio": "^0.1.2"
  },
  "devDependencies": {
    "babel": "^4.7.8",
    "babelify": "^5.0.4",
    "browserify": "^9.0.3",
    "cssnext": "^1.1.0",
    "watchify": "^2.4.0"
  }
}
